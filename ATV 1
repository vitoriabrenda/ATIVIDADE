python
print("Hello World!")

a = 8
b = 4

soma = a + b
subtracao = a - b
multiplicacao = a * b

print("Soma =", soma)
print("Subtração =", subtracao)
print("Multiplicação =", multiplicacao)

# Exemplo adicional
c = -78
d = 10

soma2 = c + d
subtracao2 = c - d
multiplicacao2 = c * d

print(soma2)
print("Soma =", soma2)
print("Subtração =", subtracao2)
print("Multiplicação =", multiplicacao2)
